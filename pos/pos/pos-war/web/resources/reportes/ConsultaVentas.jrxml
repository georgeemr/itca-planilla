	<queryString><![CDATA[select b.nom_agencia as cod_sucursal,
       sum(cantidad) as cant_pares,
       sum(monto_venta) as monto_vta
  from ventas_diarias a,
       agencias b
 where a.cod_cia = b.cod_cia
   and b.cod_agencia = decode($P{CONDICION}, '0', b.cod_agencia, $P{CONDICION})
   and a.cod_agencia = b.cod_agencia
   and trunc(a.fecha) between to_date( $P{FECHA_INICIO} , 'dd/MM/yyyy')
   and to_date($P{FECHA_FIN} , 'dd/MM/yyyy')
   and a.cod_cia = $P{CIA}
 group by b.nom_agencia]]></queryString>

