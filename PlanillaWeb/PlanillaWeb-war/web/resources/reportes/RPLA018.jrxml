	<queryString>
		<![CDATA[select
    h.nom_comercial,
    (tot_debenga +vhr_noche) - (vh_xsencilla + vh_xdoble + prestaciones + vh_x250 + vh_x150) - nvl(f.antipag,0) salarioxdias,
    d_laborados,
    ch_xsencilla + ch_xdoble + ch_x250 + ch_x150 cantidad_extra,
    vh_xsencilla + vh_xdoble + vh_x250 + vh_x150 valor_extra,
    prestaciones,
    deducciones,
    vhr_noche valor_noct,
    chr_noche cantidad_noct,
    liq_recibir,
    f.cod_emp cod_emp,
    d.num_tarjeta,
    d.cod_empref cod_ref,
    decode(a.numero_valor,0,'',(a.numero_texto || decode(a.numero_valor,1,' MILLON ',' MILLONES ' )))|| decode(b.numero_valor,0,'',(b.numero_texto || ' MIL ' )) || decode(c.numero_valor,0,'',c.numero_texto)|| decode(substr(to_char(c.numero_valor), length(to_char(c.numero_valor)),1),'1','0','')|| decode(c.numero_valor,0,'', ' ')|| decode(round(((tot_debenga)-trunc((tot_debenga),0))*100,0),0,'EXACTOS', to_char(round(((tot_debenga)- trunc((tot_debenga),0))*100,0),'00')||'/100'|| ' DOLARES') descripcion,
    e.nom_depto,
    d.apellidos,
    d.nombres,
    f.cod_cia cod_cia,
    f.anio anio,
    f.mes mes,
    f.num_planilla num_planilla,
    d.cedula,
    d.apellidos||','||d.nombres nom_emp,
    g.nom_puesto,
    g.nom_puesto||', '||e.nom_depto deptopuesto,
    d.salario_base,
    f.bonificacion,
    e.nom_depto depto1,
    f.cod_tipopla,
    nvl(f.antipag,0) complemento,
    f.cod_sucursal,
    f.cod_seccion,
    f.cod_depto,
    d.cod_puesto,
    p.fecha_inicial,
    p.fecha_final,
    p.fecha_pago
from
    cias h,
    planilla f,
    cardinal a,
    cardinal b,
    cardinal c,
    empleados d,
    departamentos e,
    puestos g,
    programacion_pla p
 where
       f.cod_cia = h.cod_cia
    and a.numero_valor = (floor(((abs(tot_debenga)))/1000000))
    and b.numero_valor = (floor(((abs(tot_debenga)))/1000)) - ((floor(((abs(tot_debenga)))/1000000))*1000)
    and c.numero_valor = (trunc((abs(tot_debenga)) - trunc((abs(tot_debenga)),-3),0))
    and f.cod_cia = $P{pCia}
    and f.anio = $P{pAnio}
    and f.mes = $P{pMes}
    and f.num_planilla = $P{pNumPla}
    and f.cod_tipopla = $P{pCodPla}
    and d.cod_cia = f.cod_cia
    and d.cod_emp = f.cod_emp
    and e.cod_cia = f.cod_cia
    and e.cod_depto = f.cod_depto
    and g.cod_cia = d.cod_cia
    and g.cod_puesto = d.cod_puesto
    and d.cod_emp = decode($P{pEmp},999999,d.cod_emp,$P{pEmp})
    and nvl(e.cod_depto,0) = decode($P{pCodDepto},0,nvl(e.cod_depto,0),$P{pCodDepto})
    --and nvl(f.cod_sucursal,'0') = decode(:sucursal,'0',nvl(f.cod_sucursal,'0'),:sucursal)
    --and nvl(f.cod_seccion,0) = decode(:seccion,'0',nvl(f.cod_seccion,0),:seccion)
    and p.anio = f.anio
    and p.mes = f.mes
    and p.cod_cia = f.cod_cia
    and p.num_planilla = f.num_planilla
    and p.cod_tipopla = f.cod_tipopla
order by
    f.cod_sucursal,
    f.cod_depto,
    f.cod_seccion,
    d.num_tarjeta,
    d.cod_emp]]>
	</queryString>
